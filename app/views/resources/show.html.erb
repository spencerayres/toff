<p id="notice"><%= notice %></p>

<div class="center">
    <div class="row">
      <% @resource.each do |resource| %>
        <div class="col-md-3">
          <div class="thumbnail">
            <h3> <%= resource.name %></h3>

          </div>
      </div>
    <% end %>
  </div>
</div>

<p>
  <strong>Url:</strong>
  <%= @resource.url %>
</p>

<p>
  <strong>Category:</strong>
  <%= @resource.category %>
</p>

<p>
  <strong>Date:</strong>
  <%= @resource.date %>
</p>

<p>
  <strong>Order:</strong>
  <%= @resource.order %>
</p>

<%= link_to 'Edit', edit_resource_path(@resource) %> |
<%= link_to 'Back', resources_path %>
